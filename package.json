{
   "name": "full-stack-todo",
   "version": "0.0.0",
   "description": "",
   "main": "dist/server.js",
   "scripts": {
      "start": "node dist/server.js",
      "dev": "concurrently \"npm run dev:server\" \"npm run dev:web-client\"",
      "dev:server": "nodemon --exec ts-node src/server.ts",
      "dev:web-client": "npm run serve --prefix web-client",
      "generate:development-seeds": "ts-node src/components/database/seeds/index.ts",
      "generate:production-seeds": "node dist/database/seeds/index.js",
      "build:source": "tsc",
      "build:web-client": "npm run build --prefix web-client",
      "build:production": "concurrently \"npm run build:source\" \"npm run build:web-client\""
   },
   "repository": {},
   "keywords": [],
   "author": "Iris Gwen J. Caguiat",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/risuu707/full-stack-todo/issues"
   },
   "homepage": "https://github.com/risuu707/full-stack-todo#readme",
   "dependencies": {
      "axios": "^0.20.0",
      "body-parser": "^1.19.0",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.25"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.7",
      "@types/express": "^4.17.8",
      "@types/node": "^8.0.29",
      "concurrently": "^5.3.0",
      "nodemon": "^2.0.4",
      "prettier": "^2.1.1",
      "ts-node": "3.3.0",
      "typescript": "^4.0.2"
   }
}
